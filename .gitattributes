# Incluir Python na contagem
*.py linguist-detectable=true linguist-language=Python
backend/scripts/**/*.py linguist-language=Python

# Incluir SQL na contagem
*.sql linguist-detectable=true linguist-language=SQL
backend/migrations/**/*.sql linguist-language=SQL

# Incluir JSON na contagem
*.json linguist-detectable=true linguist-language=JSON

# Incluir Markdown na contagem
*.md linguist-detectable=true linguist-language=Markdown

# Excluir bin√°rios e gerados
*.sqlite linguist-detectable=false
*.png linguist-detectable=false
*.jpg linguist-detectable=false
*.jpeg linguist-detectable=false
node_modules/** linguist-vendored=true
__pycache__/** linguist-vendored=true

